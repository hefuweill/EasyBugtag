<idea-plugin>
    <id>com.souche.bugtag.plugin.bugtag</id>
    <name>SoucheBugtag</name>
    <version>0.0.1</version>
    <vendor email="hefuwei@souche.com" url="http://www.yourcompany.com">大搜车</vendor>

    <description>查看Bugtag相关内容</description>
    <idea-version since-build="162.3871" until-build="173.*"/>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow id="Info" icon="/icon.png" anchor="bottom" canCloseContents="true" factoryClass="com.souche.bugtag.StackTraceCompute" secondary="false"/>
        <toolWindow id="SimpleBugtag" icon="/icon.png" anchor="bottom" canCloseContents="true" factoryClass="com.souche.bugtag.ToolFactoryCompute" secondary="false"/>
        <applicationConfigurable instance="com.souche.bugtag.Setting"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>